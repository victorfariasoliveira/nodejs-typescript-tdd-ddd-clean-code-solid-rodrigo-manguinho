{
  "name": "Express-with-TDD",
  "version": "1.0.0",
  "description": "Repo para conter uma API criada sobre o NodeJS com desenvolvimento orientado a testes",
  "main": "index.js",
  "scripts": {
    "start": "sucrase-node src/main/server.ts",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorfariasoliveira/Express-with-TDD.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/victorfariasoliveira/Express-with-TDD/issues"
  },
  "homepage": "https://github.com/victorfariasoliveira/Express-with-TDD#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/mongodb": "^3.5.2",
    "@types/node": "^13.9.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "sucrase": "^3.12.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "express": "^4.17.1",
    "validator": "^12.2.0"
  }
}
